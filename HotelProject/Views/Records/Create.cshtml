@model HotelData.ViewModels.RecordManageViewModels



<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body>
    @using (Html.BeginForm("Create", "Records", FormMethod.Post)) 
    {
        @Html.AntiForgeryToken()
        //@Html.ValidationSummary(true, "", new { @class = "text-danger" })

        <div class="form-horizontal">
            <p>Номер комнаты: @ViewBag.Id</p>
